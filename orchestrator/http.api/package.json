{
  "name": "http.api",
  "version": "0.4.105",
  "description": "",
  "author": "Philip Choi",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "bump": "$(npm bin)/bump --tag --commit && git push && git push --tags",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copyawscerts": "copyfiles docker/aws/* dist/",
    "doc": "npx compodoc -p tsconfig.json -s",
    "docker:start:dev": "docker-compose build && docker-compose up",
    "docker:start:prod": "docker-compose up -d",
    "docker:end:dev": "docker-compose down",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:dev:1": "ts-node-dev --respawn --transpile-only --exit-child --watch src ./src/main.ts",
    "start:dev:2": "nodemon -L -w src --exec 'ts-node-dev' src/main.ts | pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --runInBand --detectOpenHandles --watch",
    "test:cov": "jest --runInBand --detectOpenHandles --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.9",
    "@grpc/proto-loader": "^0.6.9",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.4.2",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/platform-fastify": "^8.4.1",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "daemonix": "^3.0.3",
    "fastify-compress": "^4.0.1",
    "fastify-helmet": "^7.0.1",
    "fastify-jwt": "^4.1.3 ",
    "fastify-plugin": "^3.0.1",
    "fastify-swagger": "^5.0.0",
    "grpc": "^1.24.11",
    "kafkajs": "^1.16.0",
    "mongoose": "^6.2.7",
    "nest-router": "^1.0.9",
    "nestjs-i18n": "^8.2.2",
    "nestjs-pino": "^2.5.0",
    "pino-http": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/got": "^9.6.12",
    "@types/jest": "^27.4.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "mongodb-memory-server": "^8.4.1",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.4",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2",
    "version-bump-prompt": "^6.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "src/core/",
      "src/main.ts",
      "src/fastify.bootstrap.ts",
      "src/dev.bootstrap.ts",
      "src/daemonix-app.ts",
      "src/api/components/",
      "src/api/v1/vendor/idemia/models/interfaces/gips/index.ts",
      "src/api/v1/components/",
      "src/api/v1/vendor/facade/",
      "src/api/v1/vendor/idemia",
      "src/api/v1/vendor/onfido",
      "src/api/v1/vendor/plaid",
      "src/api/v1/vendor/security"
    ],
    "coveragePathIgnorePatterns": [
      "src/core/",
      "src/api/v1/vendor/facade/",
      "src/api/v1/vendor/idemia",
      "src/api/v1/vendor/onfido",
      "src/api/v1/vendor/plaid",
      "src/api/v1/vendor/security",
      "src/core/billing"
    ],
    "verbose": true
  }
}
